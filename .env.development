# Development Environment for DeepWebAI
NODE_ENV=development
APP_URL=
HOST=localhost
PORT=8000

# Database — Neon PostgreSQL (pooling/SSL)
DATABASE_URL=
SKIP_DB_CONNECTION=false
SKIP_MIGRATIONS=false

# Auth (NextAuth.js)
JWT_SECRET=
NEXTAUTH_SECRET=

# OAuth (Google zorunlu)
OAUTH_GOOGLE_ID=
OAUTH_GOOGLE_SECRET=
OAUTH_REDIRECT_URL=
OAUTH_REDIRECT_URL_LOCAL=

# (İsteğe bağlı sağlayıcılar)
OAUTH_GITHUB_ID=
OAUTH_GITHUB_SECRET=
OAUTH_DISCORD_ID=
OAUTH_DISCORD_SECRET=

# Feature flags
FEATURE_FLAGS_PROVIDER=growthbook
GROWTHBOOK_API_HOST=https://cdn.growthbook.io
GROWTHBOOK_CLIENT_KEY=

# Frontend (Vite)
VITE_API_URL=http://localhost:8000
VITE_ENV=development
VITE_ENABLE_ANALYTICS=true
PLAUSIBLE_DOMAIN=localhost

# CORS
CORS_ORIGIN="http://localhost:5173"
CORS_CREDENTIALS=true

# SMTP (Zoho)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_FROM_EMAIL=
SMTP_FROM_NAME="DeepWebX AI"

# Opsiyonel
OPENAI_API_KEY=
