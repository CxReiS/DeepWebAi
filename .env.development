# Development Environment for DeepWebAI
# Bu dosya development için basit bir setup sağlar

# Database - PostgreSQL olmadan test için 
# Migration'ları skip edebiliriz
DATABASE_URL="postgresql://user:pass@localhost:5432/test_db"
SKIP_DB_CONNECTION=true

# Basic configuration
NODE_ENV="development"
HOST="localhost"
PORT=8000

# Dummy API keys for testing (çalışan değerler)
OPENAI_API_KEY="sk-test-key-not-real"
JWT_SECRET="development-jwt-secret-32-characters-long-minimum"
LUCIA_SECRET="development-lucia-secret-32-characters-long-minimum"

# Feature flags - GrowthBook (working values)
FEATURE_FLAGS_PROVIDER="growthbook"
GROWTHBOOK_CLIENT_KEY="sdk-CijMaMxaByGXrUoN"
GROWTHBOOK_API_KEY="your-growthbook-api-key"

# File upload
UPLOAD_DIR="./uploads"
MAX_FILE_SIZE=52428800

# CORS
CORS_ORIGIN="http://localhost:3000"
CORS_CREDENTIALS=true

# Skip database heavy operations in development
SKIP_MIGRATIONS=true
SKIP_ANALYTICS=true
SKIP_SENTRY=true
