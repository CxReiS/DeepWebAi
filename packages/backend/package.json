{
  "name": "backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "node src/dev-server.js",
    "dev:full": "tsx src/server.ts",
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit --skipLibCheck",
    "typecheck": "tsc --noEmit",
    "db:migrate": "npx tsx ../../scripts/migrate.ts",
    "db:seed": "npx tsx ../../scripts/seed.ts",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@deepwebai/caching": "workspace:*",
    "@deepwebai/file-processing": "workspace:*",
    "@deepwebai/shared-types": "workspace:*",
    "@elysiajs/cors": "1.3.3",
    "@neondatabase/serverless": "1.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.15.5",
    "ably": "2.10.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "elysia": "1.3.8",
    "elysia-helmet": "3.0.0",
    "ethers": "^6.9.0",
    "feature-flags": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^3.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@sentry/node": "^8.55.0",
    "@sentry/profiling-node": "^8.55.0",
    "@sentry/types": "^8.55.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.2.0",
    "@types/uuid": "^9.0.8",
    "kill-port": "^2.0.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "5.5.2",
    "vitest": "^1.5.0"
  },
  "license": "Apache-2.0"
}
