# Neon Database Configuration
DATABASE_URL="postgresql://username:password@ep-xxx.us-east-1.aws.neon.tech/deepweb_ai?sslmode=require"
NEON_DATABASE_URL_DEV="postgresql://username:password@ep-xxx.us-east-1.aws.neon.tech/deepweb_ai?sslmode=require"
NEON_DATABASE_URL_STAGING="postgresql://username:password@ep-xxx.us-east-1.aws.neon.tech/deepweb_ai?sslmode=require"
NEON_DATABASE_URL_PROD="postgresql://username:password@ep-xxx.us-east-1.aws.neon.tech/deepweb_ai?sslmode=require"

# Neon API Configuration
NEON_API_KEY="your_neon_api_key_here"
NEON_PROJECT_ID="your_neon_project_id_here"

# Server Configuration
NODE_ENV="development"
PORT=3001
HOST="0.0.0.0"

# JWT Configuration
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Nextauth Auth Configuration
NEXTAUTH_SECRET="your-nextauth-secret-key-change-in-production"

# OAuth Configuration
DISCORD_CLIENT_ID="your_discord_client_id"
DISCORD_CLIENT_SECRET="your_discord_client_secret"
DISCORD_REDIRECT_URI="http://localhost:3001/auth/discord/callback"

GITHUB_CLIENT_ID="your_github_client_id"
GITHUB_CLIENT_SECRET="your_github_client_secret"
GITHUB_REDIRECT_URI="http://localhost:3001/auth/github/callback"

# AI Provider API Keys
OPENAI_API_KEY="sk-your-openai-api-key"
ANTHROPIC_API_KEY="your-anthropic-api-key"
DEEPSEEK_API_KEY="your-deepseek-api-key"
GEMINI_API_KEY="your-gemini-api-key"

# Ably WebSocket Configuration
ABLY_API_KEY="your_ably_api_key"
ABLY_APP_ID="your_ably_app_id"

# Storage Configuration
STORAGE_TYPE="local" # local, s3, minio
STORAGE_PATH="/uploads"
MAX_FILE_SIZE="10485760" # 10MB in bytes

# S3/MinIO Configuration
S3_ENDPOINT="localhost:9000"
S3_ACCESS_KEY="minioadmin"
S3_SECRET_KEY="minioadmin123"
S3_BUCKET="deepweb-ai-files"
S3_REGION="us-east-1"
S3_USE_SSL="false"

# Redis Configuration
REDIS_URL="redis://localhost:6379"
REDIS_PREFIX="deepweb:"

# Rate Limiting
RATE_LIMIT_WINDOW_MS="900000" # 15 minutes
RATE_LIMIT_MAX_REQUESTS="100"
RATE_LIMIT_SKIP_FAILED_REQUESTS="true"

# CORS Configuration
CORS_ORIGIN="http://localhost:3000"
CORS_CREDENTIALS="true"

# Monitoring Configuration
SENTRY_DSN="your_sentry_dsn_here"
SENTRY_ENVIRONMENT="development"

# Logging Configuration
LOG_LEVEL="debug" # error, warn, info, debug
LOG_FORMAT="json" # json, simple
LOG_FILE="logs/app.log"

# Security
BCRYPT_ROUNDS="12"
HELMET_ENABLED="true"
TRUST_PROXY="false"

# Feature Flags
ENABLE_SWAGGER="true"
ENABLE_METRICS="true"
ENABLE_WEBHOOKS="false"

# AI Configuration
DEFAULT_MODEL="gpt-3.5-turbo"
MAX_TOKENS_PER_REQUEST="4096"
TEMPERATURE="0.7"
